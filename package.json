{
  "name": "discord-monad-nft-verify-bot",
  "version": "1.0.0",
  "description": "A Discord bot that verifies NFT ownership on Monad and other EVM chains with staking support",
  "main": "monad-bot.js",
  "scripts": {
    "start": "node monad-bot.js",
    "monad": "node monad-bot.js",
    "test-monad": "node test-monad.js",
    "dev": "nodemon monad-bot.js",
    "dev-monad": "nodemon monad-bot.js",
    "test": "jest",
    "lint": "eslint .",
    "docker:build": "docker build -t discord-monad-nft-verify-bot .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy:docker": "./scripts/deploy.sh docker",
    "deploy:vps": "./scripts/deploy.sh vps",
    "logs": "./scripts/deploy.sh logs"
  },
  "keywords": [
    "discord",
    "bot",
    "wallet",
    "verification",
    "opensea",
    "crypto",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "alchemy-sdk": "^3.3.1",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.10",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "keccak": "^3.0.4",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "puppeteer": "^24.16.2",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/discord-wallet-verify-bot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/discord-wallet-verify-bot/issues"
  },
  "homepage": "https://github.com/yourusername/discord-wallet-verify-bot#readme"
